{
  "name": "ngx-dnd",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "lint.all": "npm run lint.dnd && npm run lint.demo",
    "lint.dnd": "ng lint @beyerleinf/ngx-dnd",
    "lint.demo": "ng lint demo",
    "test.all": "npm run test.dnd",
    "test.dnd": "ng test @beyerleinf/ngx-dnd --code-coverage",
    "build.all": "npm run build.dnd && npm run build.demo",
    "build.dnd": "ng build @beyerleinf/ngx-dnd --prod && npm run copy-styles && npm run copy-readme && npm run copy-license",
    "build.demo": "ng build demo --prod",
    "codecov": "codecov -f coverage/*.json",
    "copy-styles": "cp ./projects/beyerleinf/ngx-dnd/styles.css ./dist/lib/",
    "copy-readme": "cp README.md ./dist/lib/",
    "copy-license": "cp LICENSE.md ./dist/lib/",
    "publish.dnd": "npm run build.dnd && npm publish dist/lib --access public"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "^6.0.4",
    "@angular/common": "^6.0.4",
    "@angular/compiler": "^6.0.4",
    "@angular/core": "^6.0.4",
    "@angular/forms": "^6.0.4",
    "@angular/http": "^6.0.4",
    "@angular/platform-browser": "^6.0.4",
    "@angular/platform-browser-dynamic": "^6.0.4",
    "@angular/router": "^6.0.4",
    "angular-prism": "^0.1.20",
    "core-js": "^2.5.7",
    "rxjs": "^6.2.1",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.7.1",
    "@angular-devkit/build-ng-packagr": "~0.6.8",
    "@angular/cli": "~6.0.8",
    "@angular/compiler-cli": "^6.0.4",
    "@angular/language-service": "^6.0.4",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~10.5.1",
    "codecov": "^3.0.2",
    "codelyzer": "~4.4.2",
    "jasmine-core": "~3.1.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.2",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-firefox-launcher": "^1.1.0",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^1.1.0",
    "ng-packagr": "^4.0.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tsickle": ">=0.29.0",
    "tslib": "^1.9.2",
    "tslint": "~5.10.0",
    "typescript": "^2.7.2"
  },
  "greenkeeper": {
    "ignore": [
      "typescript"
    ]
  }
}
