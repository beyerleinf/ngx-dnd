{
  "name": "ngx-dnd",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "e2e": "ng e2e",
    "lint": "ng lint",
    "test": "npm run test:dnd",
    "test:dnd": "ng test @beyerleinf/ngx-dnd --code-coverage",
    "build": "npm run build:dnd && npm run build:demo",
    "build:dnd": "ng build @beyerleinf/ngx-dnd --configuration=production && npm run copy-styles && npm run copy-readme && npm run copy-license && npm run copy-changelog",
    "build:demo": "ng build demo ---configuration=production",
    "codecov": "codecov -f coverage/*.json",
    "copy-styles": "cp ./projects/beyerleinf/ngx-dnd/styles.css ./dist/lib/",
    "copy-readme": "cp README.md ./dist/lib/",
    "copy-license": "cp LICENSE.md ./dist/lib/",
    "copy-changelog": "cp ./projects/beyerleinf/ngx-dnd/CHANGELOG.md ./dist/lib",
    "publish:dnd": "npm run build:dnd && npm publish dist/lib --access public",
    "doc:dnd": "npx compodoc -p ./projects/beyerleinf/ngx-dnd/tsconfig.lib.json"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "^7.2.4",
    "@angular/common": "^7.2.4",
    "@angular/compiler": "^7.2.4",
    "@angular/core": "^7.2.4",
    "@angular/forms": "^7.2.4",
    "@angular/http": "^7.2.4",
    "@angular/platform-browser": "^7.2.4",
    "@angular/platform-browser-dynamic": "^7.2.4",
    "@angular/router": "^7.2.4",
    "core-js": "^2.6.3",
    "highlight.js": "^9.14.2",
    "ngx-bootstrap": "^3.2.0",
    "ngx-highlightjs": "^3.0.1",
    "rxjs": "^6.4.0",
    "zone.js": "^0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular-devkit/build-ng-packagr": "~0.13.0",
    "@angular/cli": "~7.3.0",
    "@angular/compiler-cli": "^7.2.4",
    "@angular/language-service": "^7.2.4",
    "@compodoc/compodoc": "^1.1.7",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.6",
    "@types/node": "^10.12.24",
    "codecov": "^3.1.0",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~3.3.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.4",
    "karma-firefox-launcher": "^1.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "ng-packagr": "^5.0.1",
    "protractor": "~5.4.2",
    "standard-version": "^4.4.0",
    "ts-node": "~8.0.2",
    "tsickle": ">=0.34.3",
    "tslib": "^1.9.3",
    "tslint": "~5.12.1",
    "typescript": "^3.1.6"
  },
  "greenkeeper": {
    "ignore": [
      "typescript"
    ]
  }
}